{
  "name": "select-vue-custom",
  "version": "0.1.0",
  "private": false,
  "main": "./dist/selectCustom.common.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "scripts": {
